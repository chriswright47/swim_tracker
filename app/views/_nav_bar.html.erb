<div class="row">
  <ul class="list-unstyled">
    <li>
      <% if current_user %>
        <%= link_to 'Log out', log_out_path, {:class => 'btn'} %>
        <%= link_to 'Athletes', athletes_path, {:class => 'btn'} %>
        <%= link_to 'Meets', meets_path, {:class => 'btn'} %>
      <% else %>
        <%= link_to 'Log in', log_in_path, {:class => 'btn'} %>
        <%= link_to 'Create account', sign_up_path, {:class => 'btn'} %>
      <% end %>
    </li>
  </ul>
</div>
